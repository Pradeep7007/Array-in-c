#include<stdio.h>
int main(){
    int a,b,n;
    printf("element to search:");
    scanf("%d",&a);
    printf("size:");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n;i++){
            if(a==arr[i]){
                printf("%d ",i);
            }
        }
    }
